rsource '../f2f4f7mp1/Kconfig.clk'

config CLOCK_PLL_M
    int "M: PLLIN division factor" if CUSTOM_PLL_PARAMS
    default 4 if CPU_FAM_F7

config CLOCK_PLL_N
    int "N: PLLIN multiply factor" if CUSTOM_PLL_PARAMS
    default 216 if BOARD_HAS_HSE
    default 108
    range 50 432
